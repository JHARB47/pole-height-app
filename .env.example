# Copy this file to .env (local only) and fill in values
# Vite exposes vars prefixed with VITE_

# Application Configuration
VITE_APP_NAME="Pole Height Calculator"
VITE_APP_VERSION="1.0.0"

# Build Configuration (for development)
VITE_BUILD_TARGET="production"
VITE_DEBUG_MODE="false"

# API Configuration (if needed in future)
# VITE_API_BASE_URL="https://api.example.com"

# Feature Flags
VITE_ENABLE_GEOSPATIAL_IMPORT="true"
VITE_ENABLE_HELP_SYSTEM="true"
VITE_ENABLE_FORMAT_PREFERENCES="true"

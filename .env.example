## Copy this file to .env (local only) and customize values as needed
# Note: Vite only exposes vars prefixed with VITE_

# Application
VITE_APP_NAME="Pole Plan Wizard"
VITE_APP_VERSION="0.0.0"

# Build/runtime
VITE_BUILD_TARGET="development"   # or "production"
VITE_DEBUG_MODE="false"

# API (future)
# VITE_API_BASE_URL="https://api.example.com"

# Feature flags
VITE_ENABLE_GEOSPATIAL_IMPORT="true"
VITE_ENABLE_HELP_SYSTEM="true"
VITE_ENABLE_FORMAT_PREFERENCES="true"

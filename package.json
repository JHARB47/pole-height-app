    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shpjs": "^4.0.4",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "buffer": "^6.0.3",
  "@stackbit/cms-git": "^1.0.35",
  "@stackbit/types": "^2.1.14",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
